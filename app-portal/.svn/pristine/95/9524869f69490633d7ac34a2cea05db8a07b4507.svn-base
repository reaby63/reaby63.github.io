@import 'tailwindcss/base';
@import 'tailwindcss/components';


.carousel-codepen{
	margin:0 auto;
	text-align: center;
	overflow: hidden;
	height:100px;
	max-width: auto;
	height: 282px;
    padding-top: 77px;
	img{
		max-width: auto;
		width: auto;
		height: 282px;
	}
}

@media(min-width:769px){
	.carousel-codepen{
		margin:0 auto;
		width: 100%;
		max-width: 1200px;
		text-align: center;
		overflow: hidden;
		height:auto;
		padding-top: 0px;
		img{
			width: 100%;
			height: auto;
		}
	}
}


